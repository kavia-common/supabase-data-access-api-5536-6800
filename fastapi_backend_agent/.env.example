# Supabase Data Access API - .env.example (module-local)
# Copy to fastapi_backend_agent/.env if you plan to start the server from this directory.
# If you run from the repository root, prefer using the repo-level .env instead.

# --- Required ---
SUPABASE_URL="https://YOUR-PROJECT-REF.supabase.co"
# Prefer server-side Service Role key (do not expose publicly)
SUPABASE_SERVICE_ROLE_KEY="YOUR-SERVICE-ROLE-KEY"
# SUPABASE_ANON_KEY="YOUR-ANON-KEY"

# --- Optional ---
SUPABASE_SCHEMA="public"

# CORS settings: "*" or CSV of origins
CORS_ALLOW_ORIGINS="*"

# Logging level
LOG_LEVEL="INFO"

# Server port
APP_PORT="3001"

# Development convenience
RELOAD="true"

# --- Legacy fallback (supported by code but discouraged) ---
# SUPABASE_KEY="YOUR-LEGACY-KEY"
